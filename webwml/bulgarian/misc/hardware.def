#use wml::debian::common_tags
#use wml::debian::translation-check translation="1.3"

<bind-gettext-domain domain="others" />

# <\p> included in the where so must include that
<define-tag item whitespace=delete endtag=required>
  <p>%body
</define-tag>

# Stuff we want
<define-tag wanted whitespace=delete>
  <strong><gettext domain="others">Търси се:</gettext></strong> %attributes<br/> 
</define-tag>

# Stuff we have been offered and want to place
<define-tag wanted whitespace=delete>
  <strong><gettext domain="others">Търси се:</gettext></strong> %attributes<br/> 
</define-tag>

# Who/what project wants it
<define-tag who whitespace=delete>
  <strong><gettext domain="others">Кой:</gettext></strong> %attributes<br/> 
</define-tag>

<define-tag arch whitespace=delete>
  <strong><gettext domain="others">Архитектура:</gettext></strong> %attributes<br/> 
</define-tag>

<define-tag specs whitespace=delete>
  <strong><gettext domain="others">Спецификации:</gettext></strong> %attributes<br/> 
</define-tag>

# Always need a where tag to give </p>
<define-tag where whitespace=delete>
  <strong><gettext domain="others">Къде:</gettext></strong> %attributes</p>
</define-tag>



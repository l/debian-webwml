#use wml::debian::countries
#use wml::debian::url
#use wml::debian::common_tags
#use wml::debian::translation-check translation="1.39"

<bind-gettext-domain domain="others" />

<define-tag products whitespace=delete>
  <gettext domain="others">Продукти</gettext>
</define-tag>
<define-tag t-shirts whitespace=delete>
  <gettext domain="others">Тениски</gettext>
</define-tag>
<define-tag hats whitespace=delete>
  <gettext domain="others">шапки</gettext>
</define-tag>
<define-tag stickers whitespace=delete>
  <gettext domain="others">стикери</gettext>
</define-tag>
<define-tag mugs whitespace=delete>
  <gettext domain="others">чаши</gettext>
</define-tag>
<define-tag other_clothing whitespace=delete>
  <gettext domain="others">други дрехи</gettext>
</define-tag>
<define-tag polo_shirts whitespace=delete>
  <gettext domain="others">поло</gettext>
</define-tag>
<define-tag frisbees whitespace=delete>
  <gettext domain="others">фрийсби</gettext>
</define-tag>
<define-tag mouse_pads whitespace=delete>
  <gettext domain="others">подложки за мишка</gettext>
</define-tag>
<define-tag badges whitespace=delete>
  <gettext domain="others">баджове</gettext>
</define-tag>
<define-tag basketball_goals whitespace=delete>
  <gettext domain="others">баскетболни кошове</gettext>
</define-tag>

# Jewelry for ears, usually on ear lobes.
<define-tag earrings whitespace=delete>
  <gettext domain="others">обици</gettext>
</define-tag>

<define-tag suitcases whitespace=delete>
  <gettext domain="others">куфари</gettext>
</define-tag>

<define-tag mercentry endtag=required>\
  <p>%body
  <hr>
</define-tag>

<define-tag vendor>\
  <font size="+1"><strong>%attributes</strong></font>
  <br>
</define-tag>

<define-tag their_products>\
  <strong><products>:</strong> %attributes
  <br>
</define-tag>

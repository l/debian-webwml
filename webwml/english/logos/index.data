</define-tag>
<define-tag alt-perfect_os whitespace=delete>
  <gettext domain="others">Debian: The Perfect OS</gettext>
</define-tag>
<define-tag alt-next whitespace=delete>
  <gettext domain="others">Debian GNU/Linux: Your next Linux distribution.</gettext>
</define-tag>
<define-tag alt-powered_by_debian whitespace=delete>

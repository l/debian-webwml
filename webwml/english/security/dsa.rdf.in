#use wml::debian::recent_list
#use wml::debian::security_tags

<?xml version="1.0" encoding="$(CHARSET)"?>
<?xml-stylesheet href="dsa-rdf.css" type="text/css"?>

<rdf:RDF 
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns="http://purl.org/rss/1.0/"
  xml:lang="$(CUR_ISO_LANG)"
>

<channel rdf:about="http://www.debian.org/security/dsa.rdf">
  <title><debian_security></title>
  <link>http://security.debian.org/</link>
  <description>
<debian_security_advisories>
  </description>
  <items>
    <rdf:Seq>
<: rdf_item_list ( $(CUR_YEAR), '6', '$(ENGLISHDIR)/security', 'dsa-\d+' ); :>
    </rdf:Seq>
  </items>
</channel>

<:= get_recent_list ( $(CUR_YEAR), '6', '$(ENGLISHDIR)/security', 'rdf', 'dsa-\d+' ); :>

</rdf:RDF>

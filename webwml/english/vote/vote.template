<define-tag pagetitle>TITLE OF THE PAGE i.e. VOTE</define-tag>
<define-tag status>P|D|V|F|O</define-tag>
# meanings of the <status> tag:
# P: proposed
# D: discussed
# V: voted on
# F: finished
# O: other (or just write anything else)

#use wml::debian::template title="<pagetitle>" NOHEADER="true"
#use wml::debian::votebar

<h1><pagetitle></h1>
<h2>Proposal NNNN</h2>

<vdate/>
  <p>
  	<date>
  </p>

<vproposer/>
  <p>NAME SURNAME [<a href="mailto:a@b.c">a@b.c</a>]</p>

<vseconds/>
  <ol>
    <li>NAME SURNAME [<a href="mailto:a@b.c">a@b.c</a>]
    <li>NAME SURNAME [<a href="mailto:a@b.c">a@b.c</a>]
    ...
  </ol>

<vtext/>
  <p>
  TEXT OF THE PROPOSAL
  </p>

<vamendments/>
# any of these:
#    <em>Seconded accepted by proposer</em><br>

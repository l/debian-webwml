<define-tag reseller>\
	<preserve vend>\
	<set-var %attributes>\
	<:{
		my $var = qq/<get-var vend>/;
		if ($var eq '') {
			$str =
				"[EN:reseller:]".
				"[ES:revendedor:]".
				"[FR:revendeur:]".
				"";
			print $str;
		}
		else {
			$str = "[EN:reseller of $var:]".
				"[ES:revendedor de $var:]".
				"[FR:revendeur de $var:]".
				"";
			print $str;
		}
	}:>\
	<restore vend>
</define-tag>

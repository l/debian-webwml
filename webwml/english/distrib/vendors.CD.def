<define-tag reseller whitespace=delete>
	<preserve vend>
	<set-var %attributes>
	<:{
		my $var = qq/<get-var vend>/;
		if ($var eq '') {
			$str =
				"[EN:reseller:]".
                                "[FI:jälleenmyyjä:]".
				"[ES:revendedor:]".
				"[FR:revendeur:]".
				"[DE:Wiederverk&auml;ufer:]".
				"[HR:preprodaje:]".
				"[JA:Å¾Çä:]".
				"";
			print $str;
		}
		else {
			$str = "[EN:reseller of $var:]".
                                "[FI:$var:n jälleenmyyjä:]".
				"[ES:revendedor de $var:]".
				"[FR:revendeur de $var:]".
				"[DE:Wiederverk&auml;ufer von $var:]".
				"[HR:preprodaje $var:]".
				"[JA:$var ¤«¤é¤ÎÅ¾Çä:]".
				"";
			print $str;
		}
	}:>
	<restore vend>
</define-tag>

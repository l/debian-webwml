<define-tag reseller>\
	<preserve vend>\
	<set-var %attributes>\
	<:{
		my $var = qq/<get-var vend>/;
		if ($var eq '') {
			$str =
				"[EN:reseller:]".
				"[ES:revendedor:]".
				"";
			print $str;
		}
		else {
			$str = "[EN:reseller of $var:]".
				"[ES:revendedor de $var:]".
				"";
			print $str;
		}
	}:>\
	<restore vend>
</define-tag>

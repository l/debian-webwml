#include "$(ENGLISHDIR)/CD/vendors/vendors.CD.def"

#use wml::debian::common_tags

<bind-gettext-domain domain="others" />

# uses the following tags from vendors.CD.def:
# <vendorentry>, <vendor>, <email>, <ship>
# and the following tag from the url template included through the above:
# <URL>
# and the per-country tags (<XXc>) from the countries template, also
# included through the above


<define-tag phone whitespace=delete>
  <strong><gettext domain="others">Phone:</gettext></strong> %attributes<br />
</define-tag>
<define-tag fax whitespace=delete>
  <strong><gettext domain="others">Fax:</gettext></strong> %attributes<br />
</define-tag>
<define-tag smail whitespace=delete>
  <table border="0">
    <tr>
      <th valign=top>
        <strong><gettext domain="others">Address:</gettext></strong>
      </th>
      <td>%attributes</td>
    </tr>
  </table>
  <br />
</define-tag>



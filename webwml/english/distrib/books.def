<define-tag URL whitespace=delete>
	<strong>
	[EN:URL:]
	[FI:URL:]
	[ES:URL:]
	[FR:URL:]
	[DE:URL:]
	[HR:URL:]
	[JA:URL:]
	[ZH:URL:]
	[SV:Webbadress:]
	[NO:URL:]
	[RU:URL:]

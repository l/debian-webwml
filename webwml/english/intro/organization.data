#include "$(ENGLISHDIR)/intro/organization.tags"

<define-tag spaces>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</define-tag>
<define-tag job><LI>%attributes --</define-tag>
<define-tag genericemail>&lt;<a href="mailto:%0">%0</a>&gt;</define-tag>
# sometimes it's appropriate to differentiate members of a team based on
# who does the work right now...
<define-tag current><BR>
<spaces><EM><current_text></EM>&nbsp;</define-tag>
<define-tag currentmail><BR>
<spaces><EM><current_text></EM> %0 &lt;<A HREF="mailto:%1">%1</A>&gt;</define-tag>
# ...sometimes it's not.
<define-tag member><BR>
<spaces><EM><member_text></EM>&nbsp;</define-tag>
<define-tag membermail><BR>
<spaces><EM><member_text></EM> %0 &lt;<A HREF="mailto:%1">%1</A>&gt;</define-tag>
# we only use the chairman tag once, for techctte, I wonder why it's here.
<define-tag chairman><BR>
<spaces><EM><chair_text></EM>&nbsp;</define-tag>
<define-tag note><BR>
<EM>%attributes</EM></define-tag>


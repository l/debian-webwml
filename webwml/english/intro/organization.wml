<define-tag genericemail>\
	&lt;<a href="mailto:%0">%0</a>&gt;
</define-tag>
<define-tag current>\
	<BR>&#160;&#160;&#160;&#160;&#160;<em><current_text></em> %0
	&lt;<A HREF="mailto:%1">%1</a>&gt;
</define-tag>
<define-tag member>\
	<BR>&#160;&#160;&#160;&#160;&#160;<em><member_text></em> %0
	&lt;<A HREF="mailto:%1">%1</a>&gt;
</define-tag>
<define-tag chairman>\
	<BR>&#160;&#160;&#160;&#160;&#160;<em><chair_text></em> %0
	&lt;<A HREF="mailto:%1">%1</a>&gt;
</define-tag>
<define-container note>\
	<br><em>%body</em>
</define-container>
</define-tag>


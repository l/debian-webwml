#use wml::debian::basic
#use wml::debian::fixes_link
#use wml::debian::languages

<define-tag vulnerable>
	<ifeq "<isvulnerable>" "yes" "<FONT COLOR=FF0000>Yes</FONT>">
	<ifeq "<isvulnerable>" "no" "<FONT COLOR=00FF00>No</FONT>">
</define-tag>

	<P>
		<STRONG><FONT SIZE="+2"><pagetitle></FONT></STRONG>:
		<FONT SIZE="+1"><description></FONT>
	</P>
	<HR>
	<DL COMPACT>
		<DT>Date Reported: <DD><report_date>
		<DT>Affected Packages: <DD><packages>
		<DT>Vulnerable: <DD><vulnerable>
		<DT>For more information: <DD><moreinfo>
		<DT>Fixed in:
			<<securitybody>>
	</DL>

<:= languages() :>
#use wml::debian::footer
..securitybody>>

<define-tag headline>
<tr>
<th class="eventheader" colspan=3 align=left><strong>%0</strong></th>
</tr>
</define-tag>

<define-tag talk>
<tr>
<td align=right>%0</td>
<td align=left>%1</td>
<ifeq "%3" "" "<td align=left><strong>%2</strong></td>" "<td align=left><strong>%2</strong> (%3)</td>">
</tr>
</define-tag>

<define-tag activity>
<tr>
<td align=right>%0</td>
<ifeq "%2" "" "<td align=center colspan=2><strong>%1</strong></td>" "<td align=center colspan=2><strong>%1</strong> (%2)</td>">
</tr>
</define-tag>

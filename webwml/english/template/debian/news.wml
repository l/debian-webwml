#use wml::debian::common_translation
#use wml::debian::basic title="<news> -- <pagetitle>"
#use wml::debian::languages
#use wml::debian::ctime
<define-tag newsurl>\
	<a href="%0">%0</a>
</define-tag>
# Hint for translators: in the following, %0 is replaced by the URL
# in english the final line would look like, "<http://broken.com (dead.link)>"
<define-tag newsurldead whitespace=delete>\
	&lt;
	[EN:%0 (dead link):]
   [RO:%0 (dead link):]
   [EO:%0 (dead link):]
   [TR:%0 (dead link):]
   [FI:%0 (dead link):]
   [PT:%0 (dead link):]
   [HR:%0 (dead link):]
   [FR:%0 (dead link):]
   [NL:%0 (dead link):]
   [DE:%0 (dead link):]
   [IT:%0 (dead link):]
   [JA:%0 (dead link):]
   [PL:%0 (dead link):]
   [ES:%0 (dead link):]
   [DA:%0 (dead link):]
   [NO:%0 (dead link):]
   [ZH:%0 (dead link):]
   [SV:%0 (dead link):]
   [KO:%0 (dead link):]
   [HU:%0 (dead link):]
   [AR:%0 (dead link):]
   [RU:%0 (dead link):]
	&gt;
</define-tag>
<define-tag newsemail>\
	<a href="mailto:%0">%0</a>
</define-tag>

# DO NOT translate anything below here. If you feel there is something that needs
# to be changed write to debian-www first.

<H2><pagetitle></H2>

<P><tt><:= newsdate('<release_date>') :></tt><br>
<<mainbody>>

<ifeq "$(NOHOMELINK)" ""
      "<HR>\n<P><returnline>">
<ifeq "$(NOLANGUAGES)" "" "<:= languages() :>">
#use wml::debian::footer

..mainbody>>


#use wml::debian::common_translation
#use wml::debian::common_tags
#use wml::debian::languages
#use wml::debian::ctime

<define-tag othernews whitespace=delete>
  <gettext>Back to: other <a href="./">Debian news</a> || <a href="m4_HOME/">Debian Project homepage</a>.</gettext>
</define-tag>

<define-tag newsurldead whitespace=delete>
  <preserve url />
  <set-var url=%0 />
# '<get-var url />' is replaced by the URL and must not be translated.
# In English the final line would look like "<http://broken.com (dead.link)>"
  <gettext><get-var url /> (dead link)</gettext>
  <restore url />
</define-tag>

#use wml::debian::basic title="<news/> -- <pagetitle/>"

{#check_trans#}

<h1><pagetitle/></h1>

<p><strong><:= spokendate('<release_date>') :></strong></p>

{#mainbody#}

{#pre_footer#:
<hrlinehidden/>
<othernews/>
<hrline/>

<ifeq "$(NOLANGUAGES)" "" "
<:= languages_footer() :>
"/>
:#pre_footer#}

{#mainbody#:

#use wml::debian::common_translation
#use wml::debian::common_tags
#use wml::debian::languages

#use wml::debian::header

<body bgcolor="#ffffff" text="#000000" link="#8c4939" vlink="#45241c" alink="#ff2600">

<table width="100%" bgcolor="#ffffff" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td align="left" valign="middle">
      <a href="$(HOME)/CD/images/"><img src="$(HOME)/CD/images/pictures/cdimage.$(CUR_ISO_LANG).png" alt="Debian on CD" width="380" height="82" border="0"></a>
    </td>
    <td align="right" valign="middle">
      <a href="$(HOME)/"><img src="$(PICS)/logo-50.jpg" border="0" hspace="0" vspace="0" alt="" /></a>
      <a href="$(HOME)/"><img src="$(PICS)/debian.jpg" border="0" hspace="0" vspace="0" alt="<debianproject/>" /></a>
    </td>
  </tr>
</table>

<table width="100%" bgcolor="#e09e86" border="0" cellpadding="0" cellspacing="0">
  <tr>
    <td align="left" valign="top">
      <img src="$(HOME)/CD/images/pictures/menubar-tl.$(CUR_ISO_LANG).png" width="16" height="16" alt="" align="left" border="0" hspace="0" vspace="0">
    </td>
    <td rowspan="2" align="center" valign="middle">
      <a href="$(HOME)/CD/images/faq/"><img src="$(HOME)/CD/images/pictures/menu-faq.$(CUR_ISO_LANG).png" width="40" height="27" alt="faq ·" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/images/jigdo-cd/"><img src="$(HOME)/CD/images/pictures/menu-jigdo.$(CUR_ISO_LANG).png" width="60" height="27" alt="download with jigdo ·" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/images/http-ftp/"><img src="$(HOME)/CD/images/pictures/menu-http-ftp.$(CUR_ISO_LANG).png" width="93" height="27" alt="download via http/ftp ·" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/images/buy/"><img src="$(HOME)/CD/images/pictures/menu-buy.$(CUR_ISO_LANG).png" width="41" height="27" alt="buy pre-made images ·" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/images/netinst/"><img src="$(HOME)/CD/images/pictures/menu-netinst.$(CUR_ISO_LANG).png" width="125" height="27" alt="network install ·" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/images/pik/"><img src="$(HOME)/CD/images/pictures/menu-pik.$(CUR_ISO_LANG).png" width="40" height="27" alt="download with pik ·" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/images/misc/"><img src="$(HOME)/CD/images/pictures/menu-misc.$(CUR_ISO_LANG).png" width="50" height="27" alt="misc" border="0" align="middle" hspace="6" vspace="1"></a>
    </td>
    <td align="right" valign="top">
      <img src="$(HOME)/CD/images/pictures/menubar-tr.$(CUR_ISO_LANG).png" width="16" height="16" alt="" align="right" border="0" hspace="0" vspace="0">
    </td>
  </tr>
  <tr>
    <td align="left" valign="bottom">
      <img src="$(HOME)/CD/images/pictures/menubar-bl.$(CUR_ISO_LANG).png" width="16" height="16" alt="" align="left" border="0" hspace="0" vspace="0">
    </td>
    <td align="right" valign="bottom">
      <img src="$(HOME)/CD/images/pictures/menubar-br.$(CUR_ISO_LANG).png" width="16" height="16" alt="" align="right" border="0" hspace="0" vspace="0">
    </td>
  </tr>
</table>

<ifeq "$(NOHEADER)" "" "
<h1>$(title)</h1>
"/>

{#check_trans#}

{#body#}

<ifeq "$(NOLANGUAGES)" "" "
<:= languages() :>
"/>

<hrline/>

<table border="0" width="100%">
<tr>
  <td width="50%">
    <small>
    <ifeq "$(NOHOMELINK)" "" "
    <returnline/>
    "/>
    </small>
    <hrline/>
#use wml::debian::footer NOCOMMENTS=true
  </td>
  <td width="50%" align="right" valign="middle">
    <small><tt><b><a href="$(HOME)/CD/images/">debian_on_cd</a>:
    <br>
    <a href="$(HOME)/CD/images/faq/">faq</a> ·
    <a href="$(HOME)/CD/images/jigdo-cd/">jigdo</a> ·
    <a href="$(HOME)/CD/images/http-ftp/">http_ftp</a> ·
    <a href="$(HOME)/CD/images/buy/">buy</a> · 
    <a href="$(HOME)/CD/images/netinst/">net_install</a> ·
    <a href="$(HOME)/CD/images/pik/">pik</a> ·
    <a href="$(HOME)/CD/images/misc/">misc</a>
    </b></tt></small>
  </td>
</tr>
</table>

</body>
:}
</html>


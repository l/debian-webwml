#use wml::debian::common_translation
#use wml::debian::common_tags
#use wml::debian::languages

<define-tag-sliced debian_on_cd whitespace=delete>
  [EN:Debian on CD:]
  [FR:Debian en CD:]
</define-tag-sliced>
# note: only change the sep(arator) if it's not good for your charset
<define-tag-sliced sep whitespace=delete>
  [EN:·:]
</define-tag-sliced>
<define-tag-sliced dc_faq whitespace=delete>
  [EN:faq:]
</define-tag-sliced>
<define-tag-sliced dc_jigdo whitespace=delete>
  [EN:download with jigdo:]
  [FR:télécharger avec jigdo:]
</define-tag-sliced>
<define-tag-sliced dc_httpftp whitespace=delete>
  [EN:download via http/ftp:]
  [FR:télécharger par http/ftp:]
</define-tag-sliced>
<define-tag-sliced dc_buy whitespace=delete>
  [EN:buy pre-made images:]
</define-tag-sliced>
<define-tag-sliced dc_net whitespace=delete>
  [EN:network install:]
  [FR:installer par le réseau:]
</define-tag-sliced>
<define-tag-sliced dc_pik whitespace=delete>
  [EN:download with pik:]
  [FR:télécharger avec pik:]
</define-tag-sliced>
<define-tag-sliced dc_misc whitespace=delete>
  [EN:misc:]
  [FR:divers:]
</define-tag-sliced>

# don't translate anything below this point

#use wml::debian::header

<body bgcolor="#ffffff" text="#000000" link="#8c4939" vlink="#45241c" alink="#ff2600">

<table width="100%" bgcolor="#ffffff" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td align="left" valign="middle">
      <a href="$(HOME)/CD/images/"><img src="$(HOME)/CD/images/pictures/cdimage.$(CUR_ISO_LANG).png" alt="<debian_on_cd>" width="380" height="82" border="0"></a>
    </td>
    <td align="right" valign="middle">
      <a href="$(HOME)/"><img src="$(PICS)/logo-50.jpg" border="0" hspace="0" vspace="0" alt="" /></a>
      <a href="$(HOME)/"><img src="$(PICS)/debian.jpg" border="0" hspace="0" vspace="0" alt="<debianproject/>" /></a>
    </td>
  </tr>
</table>

<table width="100%" bgcolor="#e09e86" border="0" cellpadding="0" cellspacing="0">
  <tr>
    <td align="left" valign="top">
      <img src="$(HOME)/CD/images/pictures/menubar-tl.$(CUR_ISO_LANG).png" width="16" height="16" alt="" align="left" border="0" hspace="0" vspace="0">
    </td>
    <td rowspan="2" align="center" valign="middle">
      <a href="$(HOME)/CD/images/faq/"><img src="$(HOME)/CD/images/pictures/menu-faq.$(CUR_ISO_LANG).png" width="40" height="27" alt="<dc_faq> <sep>" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/images/jigdo-cd/"><img src="$(HOME)/CD/images/pictures/menu-jigdo.$(CUR_ISO_LANG).png" width="60" height="27" alt="<dc_jigdo> <sep>" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/images/http-ftp/"><img src="$(HOME)/CD/images/pictures/menu-http-ftp.$(CUR_ISO_LANG).png" width="93" height="27" alt="<dc_httpftp> <sep>" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/vendors/"><img src="$(HOME)/CD/images/pictures/menu-buy.$(CUR_ISO_LANG).png" width="41" height="27" alt="buy pre-made images <sep>" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/images/netinst/"><img src="$(HOME)/CD/images/pictures/menu-netinst.$(CUR_ISO_LANG).png" width="125" height="27" alt="<dc_net> <sep>" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/images/pik/"><img src="$(HOME)/CD/images/pictures/menu-pik.$(CUR_ISO_LANG).png" width="40" height="27" alt="<dc_pik> <sep>" border="0" align="middle" hspace="6" vspace="1"></a>
      <a href="$(HOME)/CD/images/misc"><img src="$(HOME)/CD/images/pictures/menu-misc.$(CUR_ISO_LANG).png" width="50" height="27" alt="<dc_misc>" border="0" align="middle" hspace="6" vspace="1"></a>
    </td>
    <td align="right" valign="top">
      <img src="$(HOME)/CD/images/pictures/menubar-tr.$(CUR_ISO_LANG).png" width="16" height="16" alt="" align="right" border="0" hspace="0" vspace="0">
    </td>
  </tr>
  <tr>
    <td align="left" valign="bottom">
      <img src="$(HOME)/CD/images/pictures/menubar-bl.$(CUR_ISO_LANG).png" width="16" height="16" alt="" align="left" border="0" hspace="0" vspace="0">

#use wml::debian::common_tags
#use wml::debian::common_translation

<bind-gettext-domain domain="other" />

<define-tag free><gettext domain="other">free</gettext></define-tag>
<define-tag non-free><gettext domain="other">non-free</gettext></define-tag>
<define-tag non-dist><gettext domain="other">not redistributable</gettext></define-tag>

<define-tag dls-result whitespace="delete">
          <preserve isfree />
          <set-var %attributes />
          <ifeq "<get-var isfree />" "no" "<font color="#FF0000"><non-free/></font>" />
          <ifeq "<get-var isfree />" "yes" "<font color="#00FF00"><free/></font>" />
          <ifeq "<get-var isfree />" "non-dist" "<font color="#FF0000"><non-dist/></font>" />
          <restore isfree />
</define-tag>

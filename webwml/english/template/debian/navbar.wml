#use wml::debian::common_translation
#use wml::debian::mirrors small="yes" usefonts="yes" fontcolor="#990000"

<div id="header">
   <div id="upperheader">
   <div id="logo">
  <a href="$(HOME)/"><img src="$(HOME)/logos/openlogo-nd-50.png" alt="" /></a>
  <a href="$(HOME)/" rel="start"><img src="$(PICS)/debian.png" alt="<debianproject/>" /></a>
  </div> <!-- end logo -->
<div id="serverselect">
<when <not "$(NOMIRRORS)"/>>
  <mirrors-serverlist/>
</when>
</div> <!-- end serverselect -->
</div> <!-- end upperheader -->

<!--UdmComment-->
<div id="navbar">
<p class="navpara">
<a href="$(INTRO)/about"><img src="$(PICS)/about.$(CUR_ISO_LANG).gif" alt="<aboutdebian/>" /></a>
<a href="$(HOME)/News/"><img src="$(PICS)/news.$(CUR_ISO_LANG).gif" alt="<news/>" /></a>
<a href="$(DISTRIB)/"><img src="$(PICS)/getting.$(CUR_ISO_LANG).gif" alt="<gettingdebian/>" /></a>
<a href="$(HOME)/support"><img src="$(PICS)/support.$(CUR_ISO_LANG).gif" alt="<support/>" /></a>
<a href="$(HOME)/devel/"><img src="$(PICS)/devel.$(CUR_ISO_LANG).gif" alt="<develcorner/>" /></a>
<a href="$(HOME)/sitemap" rel="contents"><img src="$(PICS)/sitemap.$(CUR_ISO_LANG).gif" alt="<sitemap_short/>" /></a>
<a href="$(SEARCH)"><img src="$(PICS)/search.$(CUR_ISO_LANG).gif" alt="<search/>" /></a>
</p>
</div> <!-- end navbar -->
</div> <!-- end header -->
<!--/UdmComment-->

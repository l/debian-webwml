# %0 and %1 are for font color and font size
<define-container FONTS_SANS-SERIF whitespace=delete>
    <FONT face="\
     <: if ("$(SANS_SERIF_FONT)" eq "") 
            {print "Arial,Helvetica"}
        else
            {print "$(SANS_SERIF_FONT)"} :>" %0 %1>
     %body
    </FONT>
</define-container>


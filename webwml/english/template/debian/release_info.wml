#use wml::debian::common_tags
#use wml::debian::ctime

<define-tag current_release_name>lenny</define-tag>
<define-tag current_testing_name>squeeze</define-tag>
<define-tag current_oldstable_name>etch</define-tag>

<define-tag current_release><current_release_lenny/></define-tag>

<define-tag current_release_potato>2.2r7</define-tag>
<define-tag current_release_date_potato><:=spokendate('2002-07-13'):></define-tag>

<define-tag current_release_woody>3.0r6</define-tag>
<define-tag current_release_date_woody><:=spokendate('2005-06-02'):></define-tag>
<define-tag current_release_sarge>3.1r8</define-tag>
<define-tag current_release_date_sarge><:=spokendate('2008-04-13'):></define-tag>
<define-tag current_release_etch>4.0r8</define-tag>
<define-tag current_release_date_etch><:=spokendate('2009-04-08'):></define-tag>
<define-tag current_release_lenny>5.0.4</define-tag>
<define-tag current_release_date_lenny><:=spokendate('2010-01-30'):></define-tag>

<define-tag current_release_short><: $f = "<current_release/>"; $f =~ s/r\d+$//; $f =~ s/^(\d\.\d)\.\d$/$1/; print $f :></define-tag>

<define-tag current_release_date><current_release_date_lenny/></define-tag>

<define-tag current-cd-release>5.0.4</define-tag>
<define-tag current-cd-release-dirname>5.0.4</define-tag>
<define-tag current-cd-release-filename>504</define-tag>

<define-tag packages_in_stable>25000</define-tag>

<set-var diversions:_cnt=0 />

<define-tag if-stable-release endtag=required>
<preserve release />
<set-var %attributes />
<ifneq "<current_release_name />" "<get-var release />"
	      "<enter NO<get-var release /><get-var diversions:_cnt /> />" "<enter <get-var release /><get-var diversions:_cnt /> />" />
%body
<leave />
<dump "<get-var release /><get-var diversions:_cnt />" />
<increment diversions:_cnt />
<restore release />
</define-tag>

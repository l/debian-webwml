# DO NOT translate anything below here. If you feel something needs to be changed, write to
# debian-www first

sub fixes_link 
{
  	my ( $release, $arch, $section, $package, $version ) = @_;

	if ( $release eq "not" )
	{
		$str = "<DD><notneeded>";
	}
	elsif ( $release eq "bo" )
	{
		$str = "<DD>$arch - (<in1.3>) $version";
	}
	elsif ( $release eq "hamm" )
	{
		$str = "<DD>$arch - (<in2.0>) ";
		if ( $version eq "not" )
		{
			$str .= "<notapplicable>";
		}
		elsif( $version eq "none" )
		{
			$str .= "<notavailable>";
		}
		else
		{
			if ( $section eq "" )
			{
				$str .= "$version";
			}
			else
			{
				$str .= "<A HREF=\"/Packages/stable/$section/$package.html\">";
				$str .= "$version</A>";
			}
		}
	}
	elsif ( $release eq "slink" )
	{
		$str = "<DD>$arch - (<in2.1>) ";
		if ( $version eq "not" )
		{
			$str .= "<notapplicable>";
		}
		elsif( $version eq "none" )
		{
			$str .= "<notavailable>";
		}
		else
		{
			if ( $section eq "" )
			{
				$str .= "$package-$version";
			}
			else
			{
				$str .= "<A HREF=\"/Packages/frozen/$section/$package.html\">";
				$str .= "$package-$version</A>";
			}
		}
	}
	elsif ( $release eq "potato" )
	{
		$str = "<DD>$arch - (<in2.2>) ";
		if ( $version eq "not" )
		{
			$str .= "<notapplicable>";
		}
		elsif( $version eq "none" )
		{
			$str .= "<notavailable>";
		}
		else
		{
			if ( $section eq "" )
			{
				$str .= "$package-$version";
			}
			else
			{
				$str .= "<A HREF=\"/Packages/unstable/$section/$package.html\">";
				$str .= "$package-$version</A>";
			}
		}
	}
	else
	{
		$str = '';
	}
	return $str;
}


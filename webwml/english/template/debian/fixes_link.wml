# DO NOT translate anything below here. If you feel something needs to be changed, write to
# debian-www first

<perl>

sub fixes_link 
{
  	my ( $release, $arch, $section, $package, $version ) = @_;

	if ( $release eq "not" )
	{
		$str = "<notneeded>";
	}
	elsif ( $release eq "bo" )
	{
		$str = "$arch - (<in1_3>) $version";
	}
	elsif ( $release eq "hamm" )
	{
		$str = "$arch - (<in2_0>) ";
		if ( $version eq "not" )
		{
			$str .= "<notapplicable>";
		}
		elsif( $version eq "none" )
		{
			$str .= "<notavailable>";
		}
		else
		{
			if ( $section eq "" )
			{
				$str .= "$version";
			}
			else
			{
				$str .= "<A HREF=\"http://packages.debian.org/stable/$section/$package.html\">";
				$str .= "$version</A>";
			}
		}
	}
	elsif ( $release eq "slink" )
	{
		$str = "$arch - (<in2_1>) ";
		if ( $version eq "not" )
		{
			$str .= "<notapplicable>";
		}
		elsif( $version eq "none" )
		{
			$str .= "<notavailable>";
		}
		else
		{
			if ( $section eq "" )
			{
				$str .= "$package-$version";
			}
			else
			{
				$str .= "<A HREF=\"http://packages.debian.org/frozen/$section/$package.html\">";
				$str .= "$package-$version</A>";
			}
		}
	}
	elsif ( $release eq "potato" )
	{
		$str = "$arch - (<in2_2>) ";
		if ( $version eq "not" )
		{
			$str .= "<notapplicable>";
		}
		elsif( $version eq "none" )
		{
			$str .= "<notavailable>";
		}
		else
		{
			if ( $section eq "" )
			{
				$str .= "$package-$version";
			}
			else
			{
				$str .= "<A HREF=\"http://packages.debian.org/unstable/$section/$package.html\">";
				$str .= "$package-$version</A>";
			}
		}
	}
	else
	{
		$str = '';
	}
	return $str;
}

</perl>

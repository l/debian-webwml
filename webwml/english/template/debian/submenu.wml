<define-tag menu-item endtag=required whitespace=delete>
<preserve basename>
<set-var %attributes>
[&nbsp;<ifneq <get-var WML_SRC_BASENAME> <get-var basename> <a href="<get-var basename>"> <b>><:
  $foo = <<"EOI";
%body
EOI
  print join ("", split (/\n/, $foo));
:><ifneq <get-var WML_SRC_BASENAME> <get-var basename> </a> </b>>&nbsp;]
<restore basename>
</define-tag>

<define-tag md5sums whitespace=delete>
  <preserve url />
  <set-var url=%0 />
# don't translate `<get-var url />'
  <gettext>MD5 checksums of the listed files are available in the <a href="<get-var url />">original advisory</a>.</gettext>
  <restore url />
</define-tag>


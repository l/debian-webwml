#use wml::debian::common_tags

<bind-gettext-domain domain="others" />

# <\p> included in the where so must include that
<define-tag item whitespace=delete endtag=required>
  <p>%body
</define-tag>

# Stuff we want
<define-tag wanted whitespace=delete>
  <strong><gettext domain="others">Wanted:</gettext></strong> %attributes<br/> 
</define-tag>

# Stuff we have been offered and and want to place
<define-tag wanted whitespace=delete>
  <strong><gettext domain="others">Wanted:</gettext></strong> %attributes<br/> 
</define-tag>

# Who/what project wants it
<define-tag who whitespace=delete>
  <strong><gettext domain="others">Who:</gettext></strong> %attributes<br/> 
</define-tag>

<define-tag arch whitespace=delete>
  <strong><gettext domain="others">Architecture:</gettext></strong> %attributes<br/> 
</define-tag>

<define-tag specs whitespace=delete>
  <strong><gettext domain="others">Specifications:</gettext></strong> %attributes<br/> 
</define-tag>

# Always need a where tag to give </p>
<define-tag where whitespace=delete>
  <strong><gettext domain="others">Where:</gettext></strong> %attributes</p>
</define-tag>



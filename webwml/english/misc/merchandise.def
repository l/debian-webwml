<define-tag mercentry endtag=required>\
  <p>%body
  <hr>
</define-tag>

<define-tag vendor>\
  <span class="merchtitle"><strong>%attributes</strong></span>
  <br>
</define-tag>

<define-tag their_products>\
  <strong><products>:</strong> %attributes
  <br>
</define-tag>

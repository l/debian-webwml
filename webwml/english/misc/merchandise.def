# Translations only occur in the '[EN: ... :]' strings
# DO NOT translate strings in 'ifeq' statements.

#use wml::debian::countries

# Greetings Translators!
# This is the normal language slicing stuff that you know and love.
# Note the Language: field which uses the languange_names template 
# to do its translation.
#

<define-tag URL whitespace=delete>
	<strong>
	[EN:URL:]
	[FI:URL:]
	[ES:URL:]
	[FR:URL:]
	[DE:URL:]
	[HR:URL:]
	[JA:URL:]
	[ZH:URL:]
	[SV:Webbadress:]
	[NO:URL:]
	[RU:URL:]
	:</strong> <: if(q{%0} eq "") { print "<none>";}
		else { $tmp=q{%0}; print "<a href=\"$tmp\">$tmp</a>";}
	:><br>
</define-tag>
<define-tag vendor>\
	<font size=+1><b>\
	%attributes\
	</b></font><br>
</define-tag>
<define-tag Products>\
	<strong>
	[EN:Products:]\
	[ES:Products:]\
	[FR:Products:]\
	[DE:Products:]\
	[HR:Products:]\
	[JA:Products:]\
	[ZH:Products:]\
	[SV:Produkter:]\
	[RU:Products:]\
	:</strong> %attributes<br>
</define-tag>
<define-tag t-shirt whitespace=delete>
	[EN:T Shirt:]
	[ES:T Shirt:]
	[FR:T Shirt:]
	[DE:T Shirt:]
	[HR:T Shirt:]
	[JA:T Shirt:]
	[ZH:T Shirt:]
	[SV:T-tröja:]
	[RU:T Shirt:]
</define-tag>
<define-container mercentry>\
%body
<HR>
</define-container>


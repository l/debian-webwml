#use wml::debian::template title="MIPS Port -- News" NOHEADER="yes"
#include "$(ENGLISHDIR)/ports/mips/menu.inc"

<define-tag news>
<preserve date news>
<set-var %attributes>
        <tr><td><:= spokendate ("<get-var date>") :></td>
                <td><get-var news></td></tr>
<restore date news>
</define-tag>

<h2>News</h2>
<table border=0>


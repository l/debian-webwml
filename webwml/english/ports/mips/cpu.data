<define-tag cpu>
	<preserve vendor name date speed icache dcache tlb isa specials>
	<set-var %attributes>
        <defvar date "-">
        <defvar speed "-">
        <defvar icache "-">
        <defvar dcache "-">
        <defvar tlb "-">
        <defvar isa "-">
        <defvar specials "">

	<tr><td><get-var vendor> <get-var name></td>
		<td><get-var date></td>
		<td><get-var speed></td>
		<td><get-var icache></td>
		<td><get-var dcache></td>
		<td><get-var tlb></td>
		<td><get-var isa></td>
		<td><get-var specials></td>
	</tr>

	<restore vendor name date speed icache dcache tlb isa specials>

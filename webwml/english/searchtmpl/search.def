#use wml::debian::common_tags

<bind-gettext-domain domain="search" />

<define-tag searchfor whitespace=delete>
  <gettext domain="search">Search for</gettext>
</define-tag>

<define-tag resultsperpage whitespace=delete>
  <gettext domain="search">Results per page</gettext>
</define-tag>

<define-tag outputformat whitespace=delete>
  <gettext domain="search">Output format</gettext>
</define-tag>

<define-tag outputlong whitespace=delete>
  <gettext domain="search">Long</gettext>
</define-tag>

<define-tag outputshort whitespace=delete>
  <gettext domain="search">Short</gettext>
</define-tag>

<define-tag outputurl whitespace=delete>
  <gettext domain="search">URL</gettext>
</define-tag>

<define-tag defaultquerytype whitespace=delete>
  <gettext domain="search">Default query type</gettext>
</define-tag>

<define-tag allwords whitespace=delete>
  <gettext domain="search">All Words</gettext>
</define-tag>

<define-tag anywords whitespace=delete>
  <gettext domain="search">Any Words</gettext>
</define-tag>

<define-tag searchthrough whitespace=delete>
  <gettext domain="search">Search through</gettext>
</define-tag>

<define-tag entiresite whitespace=delete>
  <gettext domain="search">Entire site</gettext>
</define-tag>

<define-tag language whitespace=delete>
  <gettext domain="search">Language</gettext>
</define-tag>

<define-tag anylanguage whitespace=delete>
  <gettext domain="search">Any</gettext>
</define-tag>

<define-tag searchresults whitespace=delete>
  <gettext domain="search">Search results</gettext>
</define-tag>

# $f number of first document on this page
# $l number of last document on this page
# $t total number of doucments found
<define-tag displaydocs whitespace=delete>
  <gettext domain="search">Displaying documents $f-$l of total <B>$t</B> found.</gettext>
</define-tag>

<define-tag noresults whitespace=delete>
  <gettext domain="search">Sorry, but search returned no results. <P>Some pages may be available in English only, you may want to try searching again and set the Language to Any.</gettext>
</define-tag>

<define-tag error-occured whitespace=delete>
  <gettext domain="search">An error occured!</gettext>
</define-tag>

<define-tag nowords whitespace=delete>
  <gettext domain="search">You should give at least one word to search for.</gettext>
</define-tag>

<define-tag poweredby whitespace=delete>
  <gettext domain="search">Powered by</gettext>
</define-tag>

<define-tag nextpage whitespace=delete>
  <gettext domain="search">Next</gettext>
</define-tag>

<define-tag prevpage whitespace=delete>
  <gettext domain="search">Prev</gettext>
</define-tag>

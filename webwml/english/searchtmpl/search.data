#include "../../english/searchtmpl/search.def"
<!--variables
#DBAddr  pgsql://udm:udm@localhost/udmsearch/
DBType pgsql
DBName udmsearch
DBUser udmsearch
DBMode  crc-multi
#IspellMode db

TrackQuery no
Clones no
-->

<!--top-->
#use wml::debian::common_translation HOME="http://www.debian.org"
$(title=<search>)
#use wml::debian::basic SUMMARY="$(SUMMARY)" PICS="http://www.debian.org/Pics" HOME="http://www.debian.org" INTRO="http://www.debian.org/intro" DISTRIB="http://www.debian.org/distrib" NOLANGUAGES=true NOMIRRORS=true
#use wml::debian::languages
#use wml::debian::language_names

<: if ("$(NOHEADER)" eq "") {
	print "<H1>$(title)</H1>\n";
	}
:>

{#searchbody#}
<br>
<center>

<FORM METHOD=GET ACTION="$A">
<table bgcolor=#eeeee0 order=1 width=100%>
<tr><td>
<BR>
<INPUT TYPE="hidden" NAME="ps" VALUE="10">
<searchfor>
<INPUT TYPE="text" NAME="q" SIZE=50 VALUE="$Q">
<INPUT TYPE="submit" VALUE="<search>"><BR>

<resultsperpage>
<SELECT NAME="ps">
<OPTION VALUE="10" SELECTED="$ps">10
<OPTION VALUE="20" SELECTED="$ps">20
<OPTION VALUE="50" SELECTED="$ps">50
</SELECT>

<outputformat>
<SELECT NAME="o">
<OPTION VALUE="0" SELECTED="$o"><outputlong>
<OPTION VALUE="1" SELECTED="$o"><outputshort>
<OPTION VALUE="2" SELECTED="$o"><outputurl>
</SELECT>

<defaultquerytype>
<SELECT NAME="m">
<OPTION VALUE="and" SELECTED="$m"><allwords>
<OPTION VALUE="or" SELECTED="$m"><anywords>
</SELECT>

<br>


<searchthrough>
<SELECT NAME="ul">
<OPTION VALUE=""            SELECTED="$ul"><entiresite>
<OPTION VALUE="http://www.debian.org/about/"    SELECTED="$ul"><aboutdebian>
<OPTION VALUE="http://www.debian.org/News/"    SELECTED="$ul"><news>
<OPTION VALUE="http://www.debian.org/distrib/"    SELECTED="$ul"><distribution>
<OPTION VALUE="http://www.debian.org/support/"    SELECTED="$ul"><support>
<OPTION VALUE="http://www.debian.org/devel/"    SELECTED="$ul"><develcorner>
</SELECT>

<language>
<SELECT NAME="lang">
<OPTION VALUE=""	SELECTED="$lang"><anylanguage>
<:
foreach $l (sort keys %langs) {
  print "<OPTION VALUE=\"$langs{$l}\"	SELECTED=\"\$lang\">$trans{$CUR_ISO_LANG}{$l}\n"
}
:>
</SELECT>


</td></tr>


</table>
</form>
</center>
<!--/top-->



<!--restop-->
<searchresults>
<small>$W</small><HR>
<displaydocs>
<!--/restop-->
<!--res-->
<DL><DT>
<b>$DN.</b> <a href="$DU" ><b>$DT</b></a> [<b>$DR</b>]<DD>
$DX...<BR>
<UL>
<li><A HREF="$DU" >$DU</A> ($DC) $DM, $DS bytes
$CL
</UL>
</DL>
<!--/res-->

<!--ftpres-->
<DL><DT>
<b>$DN.</b> <a href="$DU" ><b>$DT</b></a> [<b>$DR</b>]<DD>
<UL>
<li><A HREF="$DU" >$DU</A> ($DC) $DM, $DS bytes
$CL
</UL>
</DL>
<!--/ftpres-->

<!--resbot-->
<HR>
<CENTER>
$V
</CENTER>
<!--/resbot-->

<!--clone-->
<li><A HREF="$DU" >$DU</A> ($DC) $DM
<!--/clone-->

<!--navigator-->
<TABLE BORDER=0><TR>$NL $NB $NR</TR></TABLE>
<!--/navigator-->

<!--navleft-->
<TD><A HREF="$NH"><prevpage></A></TD>
<!--/navleft-->

<!--navleft_nop-->
<TD><FONT COLOR="#707070"><prevpage></FONT></TD>
<!--/navleft_nop-->

<!--navbar1-->
<TD><A HREF="$NH">$NP</A></TD>
<!--/navbar1-->

<!--navbar0-->
<TD>$NP</TD>
<!--/navbar0-->

<!--navright-->
<TD><A HREF="$NH"><nextpage></A></TD>
<!--/navright-->

<!--navright_nop-->
<TD><FONT COLOR="#707070"><nextpage></FONT></TD>
<!--/navright_nop-->


<!--notfound-->
<searchresults>
<small>$W</small><HR>
<CENTER>
<noresults>
</CENTER>
<HR>
<!--/notfound-->

<!--error-->
<CENTER>
<FONT COLOR="#FF0000"><error-occured></FONT>
<P><B>$E</B>
</CENTER>
<!--/error-->

<!--noquery-->
<FONT COLOR=red><nowords></FONT>
<!--/noquery-->



<!--bottom-->
<HR>
<font size="-1">
<poweredby>
<a href="http://search.mnoGo.ru/">mnoGoSearch</a></font><br>
<ifeq "$(NOHOMELINK)" ""
      "<HR>
<P><returnline>">
# <ifeq "$(NOLANGUAGES)" "" "<:= languages() :>">
<hr>
#use wml::debian::footer HOME="http://www.debian.org"
<!--/bottom-->
{#searchbody#:

#include "../../english/searchtmpl/search.def"
<!--variables
DBAddr  pgsql://www-data@/mnogo/?dbmode=cache
VarDir /org/search.debian.org/cache
LocalCharset   UTF-8
BrowserCharset UTF-8
HlBeg	<font color="000088"><b>
HlEnd	</b></font>
Limit g:language

-->

<!--top-->
#use wml::debian::common_translation HOME="http://www.debian.org"
$(title=<search>)
#use wml::debian::languages
#use wml::debian::language_names
#use wml::debian::basic SUMMARY="$(SUMMARY)" PICS="http://www.debian.org/Pics" HOME="http://www.debian.org" INTRO="http://www.debian.org/intro" DISTRIB="http://www.debian.org/distrib" NOLANGUAGES=true NOMIRRORS=true

<: if ("$(NOHEADER)" eq "") {
	print "<H1>$(title)</H1>\n";
	}
:>

{#searchbody#}
<br>
<center>

<FORM METHOD=GET ACTION="\$(Self)">
<table bgcolor=#eeeee0 border=1 width=100%>
<tr><td>
<BR>
<searchfor>
<INPUT TYPE="text" NAME="q" SIZE=50 VALUE="$&(q)">
<INPUT TYPE="submit" VALUE="<search>"><BR>

<resultsperpage>
<SELECT NAME="ps">
<OPTION VALUE="10" SELECTED="\$(ps)">10</OPTION>
<OPTION VALUE="20" SELECTED="\$(ps)">20</OPTION>
<OPTION VALUE="50" SELECTED="\$(ps)">50</OPTION>
</SELECT>

<outputformat>
<SELECT NAME="o">
<OPTION VALUE="0" SELECTED="\$(o)"><outputlong></OPTION>
<OPTION VALUE="1" SELECTED="\$(o)"><outputshort></OPTION>
<OPTION VALUE="2" SELECTED="\$(o)"><outputurl></OPTION>
</SELECT>

<defaultquerytype>
<SELECT NAME="m">
<OPTION VALUE="all" SELECTED="\$(m)"><allwords></OPTION>
<OPTION VALUE="any" SELECTED="\$(m)"><anywords></OPTION>
</SELECT>

<br>

<language>
<SELECT NAME="g">
<OPTION VALUE=""	SELECTED="\$(g)"><anylanguage></OPTION>
<:
foreach $l (sort keys %langs) {
  print "<OPTION VALUE=\"$langs{$l}\"	SELECTED=\"\\$(g)\">$trans{$CUR_ISO_LANG}{$l}</OPTION>\n";
}
:>
</SELECT>

</td></tr>


</table>
</form>
</center>
<!--/top-->



<!--restop-->
<searchresults>
<small>\$(W)</small><HR>
<displaydocs>
<!--/restop-->
<!--res-->
<DL><DT>
<b>\$(Order).</b> <a href="\$(URL)"><b>$&(Title)</b></a> [<b>\$(Score)</b>]<DD>
$&(Body)...<BR>
<UL>
<li><A HREF="\$(URL)">$(URL:40)</A>
($(Content-Type)) $(Last-Modified), $(Content-Length) bytes <br />
\$(CL)
</UL>
</DL>
<!--/res-->

<!--res-->
<DL><DT>
<b>\$(Order).</b> <a href="\$(URL)" ><b>$(URL:40)</b></a>
[<b>\$(Score)</b>]<DD>
</DL>
<!--/res-->

<!--stored-->
<DD><a href="\$(stored_href)">Cached copy</a></DD>
<!--/stored-->

<!--site_limit-->
<DD><small><a href="\$(self)\$(sitelimit_href)">All result from this site</a></small></DD>
<!--/site_limit-->


<!--resbot-->
<HR>
<CENTER>
<TABLE BORDER=0>
<TR>
<TD>Result pages: <TD>&nbsp;</TD></TD>\$(NL)\$(NB)\$(NR)
</TR>
</TABLE>
</CENTER>
<!--/resbot-->

<!--clone-->
<li><A HREF="\$(URL)">$(URL:40)</A> $(Last-Modified), $(Content-Length) bytes
<!--/clone-->


<!--navleft-->
<TD><A HREF="\$(self)\$(NH)"><prevpage></A></TD>
<!--/navleft-->

<!--navleft_nop-->
<TD><FONT COLOR="#707070"><prevpage></FONT></TD>
<!--/navleft_nop-->

<!--navbar1-->
<TD><A HREF="\$(self)\$(NH)">\$(NP)</A></TD>
<!--/navbar1-->

<!--navbar0-->
<TD>\$(NP)</TD>
<!--/navbar0-->

<!--navright-->
<TD><A HREF="\$(self)\$(nh)"><nextpage></A></TD>
<!--/navright-->

<!--navright_nop-->
<TD><FONT COLOR="#707070"><nextpage></FONT></TD>
<!--/navright_nop-->


<!--notfound-->
<searchresults>
<small>\$(W)</small><HR>
<CENTER>
<noresults>
</CENTER>
<HR>
<!--/notfound-->

<!--error-->
<CENTER>
<FONT COLOR="#FF0000"><error-occured></FONT>
<P><B>\$(E)</B>
</CENTER>
<!--/error-->

<!--noquery-->
<FONT COLOR=red><nowords></FONT>
<!--/noquery-->



<!--bottom-->
<HR>
<font size="-1">
<poweredby>
<a href="http://search.mnoGo.ru/">mnoGoSearch</a></font><br>
<ifeq "$(NOHOMELINK)" ""
      "<HR>
<P><returnline>">
# <ifeq "$(NOLANGUAGES)" "" "<:= languages() :>">
<hr>
#use wml::debian::footer HOME="http://www.debian.org"
<!--/bottom-->
{#searchbody#:

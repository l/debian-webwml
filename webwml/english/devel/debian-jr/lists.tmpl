<define-tag LIST endtag=required>
<TABLE BORDER=0 CELLPADING=5 CELLSPACING=5>
%body
</TABLE>
</define-tag>

<define-tag NAME endtag=required>
<TR VALIGN=TOP><TD><A HREF="http://packages.debian.org/%body"><B>%body</B></A></TD>
</define-tag>

<define-tag DESC endtag=required>
<TD>%body</TD></TR>
</define-tag>

<define-tag GROUP endtag=required>
<TR VALIGN=TOP><TD COLSPAN=2><B>%body</B></TD></TR>
</define-tag>

<define-tag COMMENTHEAD endtag=required>
<TR VALIGN=TOP><TD><B><I>%body</I></B></TD>
</define-tag>

<define-tag COMMENT endtag=required>
<TD><I>%body</I></TD></TR>
</define-tag>

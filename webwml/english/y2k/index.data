#include "$(ENGLISHDIR)/y2k/l10n.data"

<define-tag y2kentry>\
	<TR><TD width=15%>%0<TD width=5%>\
	<: if ("%1" eq "ALL") {print "<all>";} else {print "%1";} :>
	<TD width=5%><: print "<" . "font color=\""; :>\
	<ifeq "%2" "OK" "00BB00\"><ok>">\
	<ifeq "%2" "BAD" "FF0000\"><bad>">\
	<ifeq "%2" "OK?" "00BBBB\"><ok2>">\
	<ifeq "%2" "BAD?" "FF0000\"><bad2>">\
	<ifeq "%2" "??" "000000\"><unsure>">\
	<ifeq "%2" "Unknown" "000000\"><unknown>">\
	</font>
	<TD>\
	<ifeq "%2" "--" "%2" <URL>%3</URL>> </TR>
</define-tag>


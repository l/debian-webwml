<define-tag name_vendor whitespace=delete>
  <gettext>Vendor/Name</gettext>
</define-tag>

<define-tag name_date whitespace=delete>
  <gettext>Date announced</gettext>
</define-tag>

<define-tag name_speed whitespace=delete>
  <gettext>Clock</gettext>
</define-tag>

<define-tag name_icache whitespace=delete>
  <gettext>ICache</gettext>
</define-tag>

<define-tag name_dcache whitespace=delete>
  <gettext>DCache</gettext>
</define-tag>

<define-tag name_tlb whitespace=delete>
  <gettext>TLB</gettext>
</define-tag>

<define-tag name_isa whitespace=delete>
  <gettext>ISA</gettext>
</define-tag>

<define-tag name_specials whitespace=delete>
  <gettext>Specials</gettext>
</define-tag>

<define-tag cpu>
	<preserve vendor name date speed icache dcache tlb isa specials>
	<set-var %attributes>
        <defvar date "-">
        <defvar speed "-">
        <defvar icache "-">
        <defvar dcache "-">
        <defvar tlb "-">
        <defvar isa "-">
        <defvar specials "">

	<tr><td><get-var vendor> <get-var name></td>
		<td><get-var date></td>
		<td><get-var speed></td>
		<td><get-var icache></td>
		<td><get-var dcache></td>
		<td><get-var tlb></td>
		<td><get-var isa></td>
		<td><get-var specials></td>
	</tr>

	<restore vendor name date speed icache dcache tlb isa specials>
</define-tag>

<table>
	<tr><td><name_vendor></td>
		<td><name_date></td>
		<td><name_speed></td>
		<td><name_icache></td>
		<td><name_dcache></td>
		<td><name_tlb></td>
		<td><name_isa></td>
		<td><name_specials></td>
	</tr>


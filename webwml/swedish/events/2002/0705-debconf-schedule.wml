#use wml::debian::template title="Debiankonferens 2002 &ndash; Program"
#use wml::debian::translation-check translation="1.3"

<define-tag day>
<preserve date title>
<set-var %attributes>

<tr bgcolor="#abdcfc">
  <th colspan=2 align=center><get-var date></th>
  <th align=center><get-var title></th>
</tr>
<tr>
  <th>Tid</th>
  <th>Föredrag</th>
  <th>Föredragshållare</th>
</tr>

<restore date title>
</define-tag>


<define-tag talk>
<preserve time title speaker email>
<set-var %attributes>

<tr>
<if <get-var speaker>
  "<td><get-var time></td>
  <td align=center><strong><get-var title></strong></td>
  <td><get-var speaker> &lt;<get-var email>&gt;</td>"

  "<td><get-var time></td>
  <td align=center><strong><get-var title></strong></td>
  <td></td>"
>
</tr>

<restore time title speaker email>
</define-tag>

<p>Detta program är preliminärt och kan ändras.</p>

<p>&nbsp;</p>

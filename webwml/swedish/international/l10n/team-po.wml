#use wml::debian::languages

<perl>
my %langs_reverse = ();
foreach $lang (keys %langs) {
  my $foo = $langs{$lang};
  $langs_reverse{$foo} = $lang;
};

if ("$(TEAM)" ne "") {
  (my $l = "$(TEAM)") =~ s/_\S+$//;
  $teampolang = ucfirst $langs_reverse{'$(TEAM)'};
  $header = "<tr><td>Paket<td>Poäng<td>Detaljer<td>PO-filer\n";
} else {
  $header = "<tr><td>Paket<td>Poäng<td>Detaljer<td>po-filer\n"; # <td>Ansvarig<td>Kommentarer\n";
}
</perl>

#use wml::debian::template title="PO-filer för Debian på varje språk"
#use wml::debian::translation-check translation="1.5"

<: print "<h2>$teampolang</h2>\n" if ("$(TEAM)" ne ""); :>


<define-tag talk>
<preserve time title speaker email>
<set-var %attributes>

<tr>
  <td><get-var time></td>
  <td align=center><strong><get-var title></strong></td>
  <td><get-var speaker> &lt;<get-var email>&gt;</td>
</tr>

<restore time title speaker email>
</define-tag>


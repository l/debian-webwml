#use wml::debian::template title="MIPS-Portierung" NOHEADER="yes"
#include "$(ENGLISHDIR)/ports/mips/menu.inc"
#use wml::debian::translation-check translation="1.12"

<define-tag cpu>
	<preserve vendor name date speed icache dcache tlb isa specials>
	<set-var %attributes>
	<if <not <get-var date>>
		<set-var date="-">>
	<if <not <get-var speed>>
		<set-var date="-">>
	<if <not <get-var icache>>
		<set-var icache="-">>
	<if <not <get-var dcache>>
		<set-var dcache="-">>
	<if <not <get-var tlb>>
		<set-var tlb="-">>
	<if <not <get-var isa>>
		<set-var isa="-">>
	<if <not <get-var specials>>
		<set-var specials="">>

	<tr><td><get-var vendor> <get-var name></td>
		<td><get-var date></td>
		<td><get-var speed></td>
		<td><get-var icache></td>
		<td><get-var dcache></td>
		<td><get-var tlb></td>
		<td><get-var isa></td>
		<td><get-var specials></td>
	</tr>

	<restore vendor name date speed icache dcache tlb isa specials>
</define-tag>





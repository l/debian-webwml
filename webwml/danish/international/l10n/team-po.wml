#use wml::debian::languages

<perl>
my %langs_reverse = ();
foreach $lang (keys %langs) {
  my $foo = $langs{$lang};
  $langs_reverse{$foo} = $lang;
};

if ("$(TEAM)" ne "") {
  (my $l = "$(TEAM)") =~ s/_\S+$//;
  $teampolang = ucfirst $langs_reverse{'$(TEAM)'};
  $header = "<tr><td>Pakke<td>Point<td>Detaljer<td>PO-filer\n";
} else {
  $header = "<tr><td>Pakker<td>Point<td>Detaljer<td>po-filer\n"; # <td>Vedligeholder<td>Kommentarer\n";
}
</perl>


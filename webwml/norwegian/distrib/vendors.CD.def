<define-tag reseller>\
	<preserve vend>\
	<set-var %attributes>\
	<:{
		my $var = qq/<get-var vend>/;
		if ($var eq '') {
			$str =
				"[EN:reseller:]".
				"[ES:revendedor:]".
				"[FR:revendeur:]".
				"[DE:Wiederverk&auml;ufer:]".
				"[HR:preprodaje:]".
				"[JA:Å¾Çä:]".
				"[NO:videreforhandler:]".
				"";
			print $str;
		}
		else {
			$str = "[EN:reseller of $var:]".
				"[ES:revendedor de $var:]".
				"[FR:revendeur de $var:]".
				"[DE:Wiederverk&auml;ufer von $var:]".
				"[HR:preprodaje $var:]".
				"[JA:$var ¤«¤é¤ÎÅ¾Çä:]".
				"[NO:videreforhandler av $var:]".
				"";
			print $str;
		}
	}:>\
	<restore vend>
</define-tag>

#   Do not translate anuthing below this line

<:
  $status[0] = q{<ct-status-unavailable>};
  $status[1] = q{<ct-status-untranslated>};
  $status[2] = q{<ct-status-beingtranslated>};
  $status[3] = q{<ct-status-outdated>};
  $status[4] = q{<ct-status-uptodate>};
  $status[5] = q{<ct-status-unchecked>};
  $status[6] = q{<ct-status-beingrevised>};
  $status[7] = q{<ct-status-obsolete>};
  $status[8] = q{<ct-status-unknown>};

  # Turn this to '1' for debugging purposes
  $debug=0;
  #$debug=1;

  $from = 'english';
  $to = lc('$(CUR_LANG)');
  $from_abr = 'en';
  $to_abr = '$(CUR_ISO_LANG)';
  $CVSWEB = 'http://cvs.debian.org';
  $TRANSWEB = 'http://cvs.debian.org/webwml/french/international/french/translations/?cvsroot=webwml';

:>

<define-tag email>\
&lt;<A href="mailto:%0">%0</A>&gt;
</define-tag>

<define-tag package>\
<A href="http://packages.debian.org/%0">%0</A>
</define-tag>

# %0 is Chinese name, %1 is English name, %2 is email
#  or
# %0 is Chinese name, %1 is English first name
# %2 is English last name, %3 is email
#  or
# %0 is Chinese name, %1 is English first name
# %3 is English middle name, %3 is English last name
# %4 is email
<define-container Entry whitespace=delete>
 <table 
# border="1" 
 width="100%" cellpadding="2">
  <if <string-eq %3 "">
    # %3 has nothing
    <prog
    <TR>
      <TD width="35%" bgcolor="#CCCCCC"> <STRONG> %0 </STRONG>
      <TD width="65%" bgcolor="#CCCCCC"> <STRONG> %1 </STRONG>
    <TR>
      <TD colspan="2" bgcolor="#CCCCCC"> <STRONG> <email %2> </STRONG>
    >
  <if <string-eq %4 "">
    # %4 has nothing
    <prog
    <TR>
      <TD width="35%" bgcolor="#CCCCCC"> <STRONG> %0 </STRONG>
      <TD width="65%" bgcolor="#CCCCCC"> <STRONG> %1 <U>%2</U> </STRONG>
    <TR>
      <TD colspan="2" bgcolor="#CCCCCC"> <STRONG> <email %3> </STRONG>
    >
    # %4 has something
    <prog
    <TR>
      <TD width="35%" bgcolor="#CCCCCC"> <STRONG> %0 </STRONG>
      <TD width="65%" bgcolor="#CCCCCC"> <STRONG> %1 %2 <U>%3</U> </STRONG>
    <TR>
      <TD colspan="2" bgcolor="#CCCCCC"> <STRONG> <email %4> </STRONG>
    >
  >
  >
  <TR>
    <TD colspan="2" valign="left">%body
 </table>
</define-container>



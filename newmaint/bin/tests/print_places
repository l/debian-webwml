import os, sys
sys.path.append(os.path.abspath(".."));

import gpg_utils

testcases = []
testcases.append('print_places("", "", "Innsbruck", "AT")')
testcases.append('print_places("2002-07-28", "", "Innsbruck", "AT")')
testcases.append('print_places("", "2002-07-28", "Innsbruck", "AT")')
testcases.append('print_places("1982-07-28", "2002-07-28", "Innsbruck", "AT")')
testcases.append('print_places("1982-07-28", "2002-07-28", "", "AT")')

for test in testcases:
    command = "output = gpg_utils.%s" % test
    exec command
    print "The result of %s is:" % test
    print output


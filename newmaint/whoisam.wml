<?
  include("config.inc");
  include("common.inc");

  function print_applicants($sql) {
  if (! ($db = open_db())) {
    return FALSE;
  }
  if (! ($query = pg_exec($db, $sql))) {
    echo "Problem with query", pg_ErrorMessage($db), "<BR>";
    pg_Close($db);
    return FALSE;
  }
  $rows = pg_NumRows($query);
  for ($i = 0 ;   $i < $rows ; $i++) {
    $row = pg_Fetch_Array($query, $i);
    echo $row["name"], " &lt;", trim($row["login"]), "@debian.org&gt;<BR>\n";
  }

  pg_Close($db);
  return TRUE;
}?>
#use wml::nmpage title="Debian New Maintainer - Application Manager List"
<H1>Debian New Maintainer - Application Manager List</H1><BR>
<P>Below is the list of Debian maintainers who are Application Managers.
<BR>
<? $sql = "SELECT * from manager";
   print_applicants($sql); ?>

#!/bin/sh

# run make on all the language directories

. `dirname $0`/../common.sh

cd $webtopdir/webwml

umask 002

time make install &> /org/www.debian.org/cron/wml_run.log

test -d $webdir/mirror/timestamps || mkdir -p -m 2775 -g debwww $webdir/mirror/timestamps
date -u > $webdir/mirror/timestamps/www-master.debian.org

echo
